{
	"manifestVersion": 1,
	"id": "ProView",
	"version": "0.0.6",
	"publisher": "mohammadirfan",
	"name": "Project View",
	"description": "Enables to get complete overview of project.",
	"public": false,
	"icons": {
		"default": "static/img/logo.png"
	},
	"categories": ["Azure Repos"],
	"targets": [
		{
		"id": "Microsoft.VisualStudio.Services"
		}
	],		
	"contributions": [
		{
		"id": "PVWidget1",
		"type": "ms.vss-dashboards-web.widget",
		"targets": [
		"ms.vss-dashboards-web.widget-catalog"
		],
		"properties": {
		"name": "Project View",
		"description": "Complete project view.",
		"catalogIconUrl": "static/img/CatalogIcon.png",
		"previewImageUrl": "static/img/preview.png",
		"uri": "static/proview-home.html",
		"supportedSizes": [
		{
		"rowSpan": 1,
		"columnSpan": 2
		}
		],
		"supportedScopes": ["project_team"]
		}
		}
		],

	"files": [
		{
		"path": "static/proview-home.html", "addressable": true
		},
		{
		"path": "sdk/scripts", "addressable": true
		},
		{
		"path": "static/img", "addressable": true
		}
	]
}
		